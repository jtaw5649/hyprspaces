{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch Hyprland",
      "type": "cppdbg",
      "request": "launch",
      "program": "/usr/bin/Hyprland",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
